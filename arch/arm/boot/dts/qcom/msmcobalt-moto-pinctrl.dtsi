&tlmm {
	/delete-node/ ts_mux;

	anx_rst_active: anx_rst_active {
		mux {
			pins = "gpio131";
			function = "gpio";
		};
		config {
			pins = "gpio131";
			drive-strength = <8>; /* 8 mA */
			bias-disable = <0>; /* no pull */
		};
	};

	anx_rst_suspend: anx_rst_suspend {
		mux {
			pins = "gpio131";
			function = "gpio";
		};
		config {
			pins = "gpio131";
			drive-strength = <2>; /* 2 mA */
			bias-pull-down; /* pull down */
		};
	};

	anx_mydp_det_active: anx_mydp_det_active {
		mux {
			pins = "gpio89";
			function = "gpio";
		};
		config {
			pins = "gpio89";
			drive-strength = <2>; /* 2 mA */
			bias-disable = <0>; /* no pull */
		};
	};

	anx_mydp_det_suspend: anx_mydp_det_suspend {
		mux {
			pins = "gpio89";
			function = "gpio";
		};
		config {
			pins = "gpio89";
			drive-strength = <2>; /* 2 mA */
			bias-pull-down; /* pull down */
		};
	};

        tmp108_irq {
                tmp108_irq_default: default {
                        config {
                                pins = "gpio80";
                                drive-strength = <2>;
                                bias-pull-up;
                        };
                };
        };

        cs35l34_reset_default: cs35l34_reset_default {
                config {
                        pins = "gpio97";
                        output-high;
                        bias-pull-up;
                        drive-strength = <2>;
                };
        };

	laser_active: laser_active {
		mux {
			pins = "gpio79";
			function = "gpio";
		};

		config {
			pins = "gpio79";
			drive-strength = <2>;
			bias-disable; /* No PULL */
		};
	};

	laser_suspend: laser_suspend {
		mux {
			pins = "gpio79";
			function = "gpio";
		};

		config {
			pins = "gpio79";
			drive-strength = <2>;
			bias-disable; /* No PULL */
		};
	};

	fsa8500_hs_det_default: fsa8500_hs_det_default {
		config {
			pins = "gpio63";
			drive-strength = <2>;
			bias-pull-up;
		};
	};

	touch_int_mux {
		touch_int_active: active {
			mux {
				pins = "gpio40";
				function = "gpio";
			};
			config {
				pins = "gpio40";
				bias-pull-up;
			};
		};

		touch_int_suspend: default {
			mux {
				pins = "gpio40";
				function = "gpio";
			};
			config {
				pins = "gpio40";
				bias-disable;
			};
		};
	};

	touch_reset_mux {
		touch_reset_active: active {
			mux {
				pins = "gpio39";
				function = "gpio";
			};
			config {
				pins = "gpio39";
				bias-pull-up;
			};
		};

		touch_reset_suspend: default {
			mux {
				pins = "gpio39";
				function = "gpio";
			};
			config {
				pins = "gpio39";
				output-low;
			};
		};
	};

	/* MODS Related */
	spi_1 {
		spi_1_active {
			config {
				drive-strength = <16>;
			};
		};

		spi_1_sleep {
			mux {
				function = "gpio";
			};

			config {
				drive-strength = <2>;
				bias-pull-down;
				input-enable;
			};
		};

		spi_1_mosi_ack: spi_1_mosi_ack {
			mux {
				pins = "gpio0";
				function = "gpio";
			};

			config {
				pins = "gpio0";
				drive-strength = <2>;
				bias-disable;
				output-low;
			};
		};

		spi_1_miso_ack: spi_1_miso_ack {
			mux {
				pins = "gpio1";
				function = "gpio";
			};

			config {
				pins = "gpio1";
				drive-strength = <2>;
				bias-disable;
				input-enable;
			};
		};
	};

	/* INT_N, BPLUS_EN, FORCE_FLASH */
	muc_attach: muc_attach@0 {
		config {
			pins = "gpio26", "gpio35", "gpio143";
			bias-disable;
		};
	};

	muc_det: muc_det@0 {
		config {
			pins = "gpio36";
			bias-disable;
			drive-strength = <16>;
		};
	};

	/* WAKE_N, RDY_N */
	muc_spi_ctrl: muc_spi_ctrl@0 {
		config {
			pins = "gpio28", "gpio95";
			bias-disable;
		};
	};
};

&pmx_mdss{
	mdss_dsi_active: mdss_dsi_active {
		mux {
			pins = "gpio8", "gpio69";
			function = "gpio";
		};

		config {
			pins = "gpio8", "gpio69";
			drive-strength = <8>; /* 8 mA */
			bias-disable = <0>; /* no pull */
		};
	};

	mdss_dsi_suspend: mdss_dsi_suspend {
		mux {
			pins = "gpio8", "gpio69";
			function = "gpio";
		};

		config {
			pins = "gpio8", "gpio69";
			drive-strength = <2>; /* 2 mA */
			bias-pull-down; /* pull down */
		};
	};
};
