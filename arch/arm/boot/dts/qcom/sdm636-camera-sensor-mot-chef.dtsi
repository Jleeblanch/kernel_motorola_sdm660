		qcom,page1 = <1 0x3d8a 2 0x7045 2 1>;
		qcom,poll1 = <1 0x3d8a 2 0x7045 2 1>;
		qcom,mem1 = <0 0x7010 2 0 1 1>;
		qcom,page2 = <1 0x3d81 2 0x01 1 1>;
		qcom,poll2 = <1 0x3d81 2 0x01 1 1>;
		qcom,mem2 = <0 0x7010 2 0 1 1>;
		qcom,page3 = <1 0x0100 2 0x1 1 1>;
		qcom,poll3 = <0 0x0100 2 0x1 1 1>;
