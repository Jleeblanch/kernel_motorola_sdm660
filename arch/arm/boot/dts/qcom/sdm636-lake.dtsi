#include "sdm636-moto-common.dtsi"
#include "dsi-panel-mot-tianma-624-1080p-vid.dtsi"
#include "dsi-panel-mot-dummy-qhd-video.dtsi"
#include "sdm636-lake-pinctrl.dtsi"
#include "sdm636-camera-sensor-mot-lake.dtsi"
#include "sdm636-lake-audio.dtsi"
#include "sdm636-egis-fps.dtsi"

/ {
	model = "lake";
	compatible = "qcom,sdm636-lake", "qcom,sdm636-moto", "qcom,sdm636";
};

&i2c_3 {
	status = "okay";

	synaptics_tcm@20 {
		compatible = "synaptics,tcm-i2c";
		reg = <0x20>;
		interrupt-parent = <&tlmm>;
		interrupts = <67 0x2008>;
		pinctrl-names = "pmx_ts_active", "pmx_ts_suspend";
		pinctrl-0 = <&touch_int_active &touch_reset_active>;
		pinctrl-1 = <&touch_int_suspend &touch_reset_suspend>;
		synaptics,irq-gpio = <&tlmm 67 0x2008>; /* IRQF_ONESHOT | IRQF_TRIGGER_LOW */
		synaptics,irq-on-state = <0>;
		synaptics,reset-gpio = <&tlmm 66 0x0000>;
		synaptics,reset-on-state = <0>;
		synaptics,reset-active-ms = <20>;
		synaptics,reset-delay-ms = <200>;
		synaptics,power-delay-ms = <200>;
		synaptics,ubl-i2c-addr = <0x20>;
	};
};

&i2c_2 {
	status = "okay";
	sx933x@28 {
		compatible = "Semtech,sx933x";
		reg = <0x28>;
		interrupt-parent = <&tlmm>;
		interrupts = <75 0>;
		Semtech,nirq-gpio = <&tlmm 75 0x00>;
		cap_vdd-supply = <&pm660l_l8>;
		Semtech,reg-num = <12>;
		Semtech,button-flag = <0x07>;
		Semtech,reg-init = <0x8020 0x3F00C0
			0x8024 0x44F
			0x8028 0x24928000
			0x802C 0x44F
			0x8030 0x24960000
			0x8034 0x44F
			0x8038 0x24B20000
			0x803C 0x44F
			0x8040 0x25920000
			0x8044 0x44F
			0x8048 0x2C920000
			0x4004 0x67 >;
	};

	/* AWINIC AW869X Haptic */
	aw869x_haptic@5B {
		compatible = "awinic,aw869x_haptic";
		reg = <0x5B>;
		reset-gpio = <&pm660_gpios 11 0>;
		irq-gpio = <&tlmm 70 0>;
		status = "okay";
	};
	/* AWINIC AW869X Haptic End */
};

&ets_fps {
	/delete-property/ egistec,gpio_ldo1p8_en;
	/delete-property/ egistec,gpio_ldo3p3_en;
	pinctrl-0 = <&ets_rst_active &ets_int_active>;
	pinctrl-1 = <&ets_rst_suspend &ets_int_suspend>;
};

&pm660_l13 {
	regulator-always-on;
	qcom,init-voltage = <1800000>;
};

&pm660l_l6 {
	regulator-always-on;
	qcom,init-voltage = <3300000>;
};

&pm660_gpios {
	/* GPIO 11 for VIB_RST_N */
	gpio@ca00 {
		qcom,mode = <1>;				/* Output */
		qcom,pull = <5>;				/* Pull up */
		qcom,vin-sel = <0>; 			/* VIN1 GPIO_LV */
		qcom,src-sel = <0>; 			/* GPIO */
		qcom,invert = <0>;				/* Invert */
		qcom,master-en = <1>;			/* Enable GPIO */
		status = "okay";
	};
};

&firmware {
	android {
		fstab {
			vendor {
				fsmgr_flags = "wait,slotselect,verify";
			};
		};
	};
};

&red_led {
	linux,name = "charging";
	/delete-property/ linux,default-trigger;
	qcom,ramp-step-ms = <100>;
	qcom,idx-len = <42>;
	qcom,duty-pcts =
		[00 05 0a 0f 14 19 1e 23 28 2d 32 37 3c 41 46 4b 50 55 5a 5f 64
		64 5f 5a 55 50 4b 46 41 3c 37 32 2d 28 23 1e 19 14 0f 0a 05 00];
};

/delete-node/ &green_led;
/delete-node/ &blue_led;

&soc{
/delete-node/ qcom,qbt1000;

	ssphy: ssphy@c010000 {
		status = "disabled";
	};

	usb_nop_phy: usb_nop_phy {
		compatible = "usb-nop-xceiv";
	};
};

&usb3 {
	dwc3@a800000 {
		usb-phy = <&qusb_phy0>, <&usb_nop_phy>;
		maximum-speed = "high-speed";
	};
};

&qusb_phy0 {
	qcom,tune2-efuse-correction = <(-9)>;
	qcom,tune2-efuse-correction-host = <(-8)>;
	qcom,qusb-phy-init-seq = <0xc0 0x80
				0x31 0x84
				0x83 0x88
				0xc7 0x8c
				0x30 0x08
				0x79 0x0c
				0x21 0x10
				0x14 0x9c
				0x9f 0x1c
				0x00 0x18>;
};

&mdss_mdp {
	qcom,mdss-pref-prim-intf = "dsi";
};

&mdss_dsi {
	hw-config = "single_dsi";
};

&mdss_dsi0 {
	qcom,dsi-pref-prim-pan = <&mot_tianma_624_1080p_vid_v0>;
	pinctrl-names = "mdss_default", "mdss_sleep";
	pinctrl-0 = <&mdss_dsi_active &mdss_te_active>;
	pinctrl-1 = <&mdss_dsi_suspend &mdss_te_suspend>;
	qcom,platform-reset-gpio = <&tlmm 53 0>;
	qcom,platform-te-gpio = <&tlmm 59 0>;
};

&pm660l_wled {
	qcom,led-strings-list = [00 01];
	qcom,en-cabc;
};

&mdss_dp_ctrl{
		status = "disabled";
};

/{
	mot_batterydata: qcom,battery-data {
		qcom,batt-id-range-pct = <15>;
		df-serialnum = "SB18C26504";

		#include "fg-gen3-batterydata-jx40atl-3010mah.dtsi"
	};
};

&pm660_haptics {
	status = "disabled";
};

