&tlmm {
	cs35l36_reset_default: cs35l36_reset_default {
		config {
			pins = "gpio63";
			output-low;
			bias-pull-up;
			drive-strength = <2>;
		};
	};
	cs35l36_irq_default: cs35l36_irq_default {
		config {
			pins = "gpio76";
			drive-strength = <2>;
			bias-pull-up;
		};
	};

	moon_irq_default: moon_irq_default {
		config {
			pins = "gpio21";
			drive-strength = <2>;
			bias-pull-up;
			input-enable;
		};
	};
	moon_reset_default: moon_reset_default {
		config {
			pins = "gpio55";
			output-high;
			bias-pull-up;
			drive-strength = <2>;
		};
	};

	usb_aud_sw_sel: usb_aud_sw_sel {
		config {
			pins = "gpio80";
			bias-pull-down;
			drive-strength = <2>;
		};
	};
};

